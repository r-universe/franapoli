[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "gep2pep"]
	path = gep2pep
	url = https://git.bioconductor.org/packages/gep2pep
	shallow = true
	registered = true
[submodule "repo"]
	path = repo
	url = https://github.com/franapoli/repo
	shallow = true
	registered = true
